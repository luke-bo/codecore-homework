<%- include("../partials/header") %>

<form class="new-cohort-form" action="/cohorts" method="POST">
    <div class="form-group">
        <label for="logoUrl">Logo URL</label>
        <input class="form-control" type="text" name="imageurl" placeholder="image.url">    
    </div>
    <div class="form-group">
        <label for="name">Team Name</label>
        <input class="form-control" type="text" name="name" placeholder="Planet Express">
    </div>
    <div class="form-group">
        <label for="members">Members</label>
        <textarea class="form-control" 
                  name="members"
                  placeholder="Fry, Leela, Hubert, Amy, Hermes, Zoidberg"
                  rows="5" 
                  cols="33">
        </textarea>
    </div>
    <input class="new-cohort-button" type="submit" value="Submit">
</form>


    
    
    
    

<%- include("../partials/footer") %>