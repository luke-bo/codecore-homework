<%- include("../partials/header") %>
<div class="container-fluid">
    <form class="new-cohort-form" action="/cohorts" method="POST">
        <div class="form-group">
            <label for="logoUrl">Logo URL</label>
            <input class="form-control" type="text" name="logoUrl" placeholder="image.url">    
        </div>
        <div class="form-group">
            <label for="name">Team Name</label>
            <input class="form-control" type="text" name="name" placeholder="Planet Express">
        </div>
        <div class="form-group">
            <label for="members">Members</label>
            <textarea placeholder= "Fry, Leela, Hubert, Amy, Hermes, Zoidberg" class="form-control" name="members" rows="5" cols="33"></textarea>
        </div>
        <input class="new-cohort-button" type="submit" value="Submit">
    </form>
</div>



    
    
    
    

<%- include("../partials/footer") %>